/**
 * The FileLogger type writes log information to a log file.
 * Unlike the mainlog feature this sets up a logger
 * with severity "debug".
 */

object FileLogger "debug-file" {
  severity = "{{ monitoring_icinga_agent__debug_log_severity }}"
  path = {{ monitoring_icinga_agent__debug_log_file }}
}
